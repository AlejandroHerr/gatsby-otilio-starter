{
  "name": "gatsby-starter-blog",
  "description": "Starter Gatsby Blog",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby-starter-blog/issues"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "classnames": "^2.2.5",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-graphql": "^1.5.0",
    "flow-typed": "^2.4.0",
    "gatsby": "^1.9.102",
    "gatsby-1-config-css-modules": "^1.0.10",
    "gatsby-1-config-extract-plugin": "^1.0.2",
    "gatsby-image": "^2.0.32",
    "gatsby-link": "^2.0.15",
    "gatsby-paginate": "^1.0.13",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-netlify": "^2.0.12",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.27",
    "gatsby-plugin-sitemap": "^2.0.8",
    "gatsby-plugin-typography": "^2.2.9",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.10",
    "gatsby-remark-images": "^3.0.9",
    "gatsby-remark-prismjs": "^3.2.5",
    "gatsby-remark-responsive-iframe": "^2.0.10",
    "gatsby-remark-smartypants": "^1.4.8",
    "gatsby-source-filesystem": "^1.5.8",
    "gatsby-transformer-json": "^1.0.16",
    "gatsby-transformer-remark": "^1.7.20",
    "gatsby-transformer-sharp": "^1.6.22",
    "node-sass": "^4.7.2",
    "normalize-css": "^2.3.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-share": "^2.0.0",
    "react-share-icons": "^1.1.2",
    "sharp": "^0.20.1",
    "slug": "^0.9.1",
    "syntax-highlighting": "https://github.com/atelierbram/syntax-highlighting.git"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-flow-react-proptypes": "^22.0.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.69.0",
    "flow-coverage-report": "^0.5.0",
    "gh-pages": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "postcss": "^6.0.19",
    "prettier": "^1.8.1",
    "react-test-renderer": "^16.3.0",
    "stylelint": "^9.1.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "dev": "yarn clean &&  gatsby develop",
    "flow": "flow check",
    "flow:typed": "flow-typed install --overwrite || true",
    "flow:coverage": "flow-coverage-report --config ./.flowcoverage.json",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public . --format=node_modules/eslint-formatter-pretty .",
    "lint:fix": "eslint --ext .js,.jsx --ignore-pattern public . --fix",
    "lint:styles": "yarn stylelint 'src/**/*.scss'",
    "test": "BABEL_DISABLE_CACHE=1 jest",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "develop": "yarn clean &&  gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rm -rf .cache/ public/",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "tests\\/[^\\/]+.+\\.spec\\.js$",
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
